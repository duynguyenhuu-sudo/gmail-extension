{
  "manifest_version": 3,
  "name": "GITS Gmail Sender",
  "version": "1.0.0",
  "description": "Send personalized outreach emails via Gmail API with attachment.",
   "icons":{
      "16": "icons/capy.png",
      "32": "icons/capy.png",
      "48": "icons/capy.png",
      "128": "icons/capy.png"
   },
  "action": {
    "default_title": "GITS Gmail Sender",
    "default_popup": "popup.html",
    "default_icon":{
      "16": "icons/capy.png",
      "32": "icons/capy.png",
      "48": "icons/capy.png",
      "128": "icons/capy.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*",
     "https://docs.google.com/*"
  ],
  "oauth2": {
    "client_id": "385178155043-j6t92gukuvh47oihp3ml794h9rhuqkhg.apps.googleusercontent.com",
    "scopes": [
        "https://www.googleapis.com/auth/gmail.send",
        "https://www.googleapis.com/auth/spreadsheets.readonly",
        "https://www.googleapis.com/auth/gmail.settings.basic"
    ]
  },
  "key":""
}